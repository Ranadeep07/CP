class Solution {
    public List<String> fizzBuzz(int n) {
         List<String> aList = new ArrayList<>();
        for (int i = 1 ; i <= n ; i++) {
        	if(i % 15 == 0) {
				aList.add("FizzBuzz");
			}
        	else if(i % 3 == 0) {
				aList.add("Fizz");
			}
			else if (i % 5 == 0) {
				aList.add("Buzz");
			}
			else {
				aList.add(Integer.toString(i));
			}
		}
        return aList;
    }
}
